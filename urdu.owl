    <!-- http://www.semanticweb.org/bisma/ontologies/2016/2/urdu.owl#حیا_سلیمان -->

    <owl:NamedIndividual rdf:about="&urdu;حیا_سلیمان">
        <rdf:type rdf:resource="&urdu;خاتون"/>
        <urdu:کردار_نمبر rdf:datatype="&xsd;string">1</urdu:کردار_نمبر>
        <urdu:کردار_کا_نام rdf:datatype="&xsd;string">برج ونگ سے منسلک ہیں۔ اپنے تحریری سفر کا آغاز انھوں نے خواتین کے ماہناموں سے کیا اور پھر ٹی وی انڈسٹری میں آ گئیں- ان کا پہلا ڈراما ‘‘وجود لاریب‘‘ انڈس ویژن سے 2005 میں آیا جس کے لئے انہوں نے بیسٹ رائٹر کا ایوارڈ لیا ۔ اس کے بعد ‘‘دام‘‘، ‘‘قید تنہائی‘‘ اور متعدد ڈرامے وہ لکھ چکی ہیں اور ان کے کئی ناول کی ڈرامائی تشکیل ہو چکی۔ ‘‘نور کا مسکن‘‘ کے نام سے ایک ریڈیو ڈراما بھی لکھا۔</urdu:کردار_کا_نام>
        <urdu:کردار_کا_نام rdf:datatype="&xsd;string">حیا_سلیمان</urdu:کردار_کا_نام>
        <urdu:تعلق rdf:resource="&urdu;جنت_کے_پتے"/>
        <urdu:ہے_ایک rdf:resource="&urdu;خاتون"/>
    </owl:NamedIndividual>
    


