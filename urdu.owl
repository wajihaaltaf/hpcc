    <!-- http://www.semanticweb.org/bisma/ontologies/2016/2/urdu.owl#جنت_کے_پتے -->

    <owl:NamedIndividual rdf:about="&urdu;جنت_کے_پتے">
        <rdf:type rdf:resource="&urdu;نام"/>
        <urdu:ناول_تفصیل rdf:datatype="&xsd;string">    جنت کے پتّے ایک سماجی اردو ناول کی کہانی نگار ناول نگار نمرہ احمد کی طرف سے ہے ۔ جنت کے پتّے ہمت، حقیقت، جدوجہد اور عقیدہ ہے۔ جنت کے پتّے کی کردار حیا سلیمان جو ل ل بی تی طا لب علم ہوتی ہے، ایک لڑکی جو ابتدائی طور پر ایک جدید لڑکی بیان کی گئ یے۔ کہانی ایک پہلو کے موضوع کے ارد گرد گھومتی ہے۔ &quot;پردہ&quot;اور کس طرح قرآن مجید کی ایک آیت زندگی ہمیشہ کے لئے تبدیل کر سکتی ہے ۔ دوسری طرف کہانی کا ایک جاسوس لڑکا جہان کی زندگی اور اس فرض کے شدید اور سخت مطالبات دکھاتی ہے ۔</urdu:ناول_تفصیل>
        <urdu:ہے_ایک rdf:resource="&urdu;ناول"/>
        <urdu:تحریر_کردہ rdf:resource="&urdu;نمرہ_احمد"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/bisma/ontologies/2016/2/urdu.owl#جہان_سکندر -->

